    <a asp-action="Create">Create New</a>
</p>
-->
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Speciality)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Availability_Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ratings)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Reviews)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Speciality)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Availability_Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ratings)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Reviews)
            </td>
            <td>
                    <a class="btn btn-primary btn-sm">SELECT</a> 
                <!--
                <a asp-action="Edit" asp-route-id="@item.Worke_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Worke_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Worke_Id">Delete</a>
                -->
            </td>
        </tr>
        }
        <script src="https://cdn.botpress.cloud/webchat/v1/inject.js"></script>
        <script src="https://mediafiles.botpress.cloud/279805d3-1648-4c00-becc-3fb8c04d195d/webchat/config.js" defer></script>
    </tbody>
</table>
